[<%= @name %>]
<% if @mirrorlist != nil -%>
mirrorlist=<%= @mirrorlist %>
<% end -%>
<% if @failovermethod != nil -%>
failovermethod=<%= @failovermethod %>
<% end -%>
<% if @priority != 'UNSET' -%>
priority=<%= @priority %>
<% end -%>
<% if @use_gpgkey_uri == true -%>
gpgkey=<%= @my_gpgkey %>
<% end -%>
<% if @my_baseurl != nil -%>
baseurl=<%= @my_baseurl %>
<% end -%>
name=<%= @description_real %>
enabled=<%= @enabled %>
gpgcheck=<%= @gpgcheck %>
