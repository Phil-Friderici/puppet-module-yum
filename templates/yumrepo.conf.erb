NameVirtualHost <% @yum_server_http_listen_ip_real %>:80
<VirtualHost <% @yum_server_http_listen_ip_real %>:80>
    ServerName <%= @yum_server %>
    ServerAlias <%= @fqdn %> <%= @hostname %>
    ServerAdmin <%= @contact_email %>

    DocumentRoot <%= @docroot %>
    <Directory <%= @docroot %> >
        Options Indexes FollowSymLinks MultiViews
        AllowOverride AuthConfig
        Order allow,deny
        allow from all
    </Directory>
</VirtualHost>
